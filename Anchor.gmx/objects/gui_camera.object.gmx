            <string>///Default Functions
if (instance_exists(class_player)) {
    //Track the mouse
    if (abs(x - mouse_x) &lt; 8 &amp;&amp; abs(y - mouse_y) &lt; 8) move = false;
    else move = true;
        
    //Follow the mouse
    if (move) {
        if (x != mouse_x) x += (mouse_x - x)/20;
        if (y != mouse_y) y += (mouse_y - y)/20;
    }
    
    //Set the view and lock on player
    view_object[0] = class_player;
    view_xview[0] = x - xBuffer;
    view_wview[0] = xRange;
    view_yview[0] = y - yBuffer;
    view_hview[0] = yRange;
    
    x = clamp(x, class_player.x - leash, class_player.x + leash);
    y = clamp(y, class_player.y - leash, class_player.y + leash);   
}
</string>
