            <string>///Collisions

//With Enemy
//notme = owner.owner;
if (/*!place_meeting(x, y, notme) &amp;&amp; !place_meeting(x, y, class_npc) &amp;&amp;*/ place_meeting(x, y, class_shadowed)) {
    hit = instance_position(x, y, class_shadowed);
    with (hit) hp -= other.damage;
    if (!piercing) instance_destroy();
    else {
        if (crit) damage = irandom_range(owner.damageAmount - damageVariance, owner.damageAmount + damageVariance);
        damage -= damageDropOff;
    }
}

//With Walls
if (place_meeting(x, y, class_wall)) {
    if (!canBounce) instance_destroy();
    else {
        if (alarm[0] &gt; room_speed * 10) alarm_set(0, dropOffTime);
        if (bounced &lt; bounceNumber) {
            targetX = irandom_range(x, initX);
            targetY = irandom_range(y, initY);
            move_towards_point(targetX, targetY, spd);
            
            image_angle = point_direction(x, y, targetX, targetY);
            initX = targetX;
            initY = targetY;
            bounced++;
        } else instance_destroy();
    }
} 
