            <string>///Init Variables
owner = instance_nearest(x, y, class_ranged);
if (owner == noone) instance_destroy();
show_debug_message(string(owner));
with (owner) alarm_set(0, initFireDelay);

damage = owner.damageAmount;
damageVariance = owner.damageVariance; 
damage = irandom_range(damage - damageVariance, damage + damageVariance);
damageDropOff = owner.damageDropOff;

critChance = owner.critChance;
critRoll = irandom_range(0, 1);
if (critRoll &lt;= critChance) crit = true;
else crit = false;
if (crit = true) damage *= 1.5;
damage = round(damage);

bounced = 0;
canBounce = owner.canBounce;
bounceNumber = owner.bounceNumber;

dropOffSpeed = owner.dropOffSpeed;
dropOffTime = owner.dropOffTime;
piercing = owner.piercing;
range = owner.range;
spd = owner.spd;

initX = x;
initY = y;

if (owner.owner == class_player) {
    move_towards_point(mouse_x, mouse_y, spd);
    image_angle = point_direction(x, y, mouse_x, mouse_y);
} else {
    move_towards_point(class_player.x, class_player.y, spd);
    image_angle = point_direction(x, y, class_player.x, class_player.y);
}
